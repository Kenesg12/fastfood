    <TfrxFDQuery Name="IBXQuery1" UserName="IBXQuery1" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT SUM(o.kolvo * o.margin)as Obsluzh,&#13;&#10;       SUM((o.kolvo * o.mdiscount)+coalesce(o.mdiscount_sum, 0))as Disc,&#13;&#10;       SUM(o.kolvo * (o.cena_sales-o.mDiscount+o.margin)-coalesce(o.mdiscount_sum, 0)) as itogo,&#13;&#10;       SUM(o.kolvo * o.cena_sales) as summa, fo.name as Foplaty&#13;&#10;FROM&#13;&#10;    documents p&#13;&#10;    INNER JOIN tbFormaOplaty fo ON (p.idformaoplaty=fo.id)&#13;&#10;    INNER JOIN d_lines o ON (p.uid=o.iddocument)&#13;&#10;    inner join d_product m on (m.id=o.product)&#13;&#10;WHERE p.bDel = 0&#13;&#10;and p.idkoord in(select id from tbKoord&#13;&#10;where idHall in(select id from tbHall&#13;&#10;where idStation = :station))&#13;&#10;and idSmeny = :sm --(select id from tbSmeny where dtend is null)&#13;&#10;GROUP BY fo.name  " PropData=""/>
