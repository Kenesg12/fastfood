  object Documents: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE DOCUMENTS'
      'SET '
      '    KOD = :KOD,'
      '    DT = :DT,'
      '    SUPPLIER = :SUPPLIER,'
      '    NUM_NAKL = :NUM_NAKL,'
      '    TOTAL_SUM = :TOTAL_SUM,'
      '    IDPEOPLES = :IDPEOPLES,'
      '    SUBEKT = :SUBEKT,'
      '    IDDOGOVOR = :IDDOGOVOR,'
      '    PROVOD = :ACCEPT'
      'WHERE'
      '    UID = :OLD_UID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    DOCUMENTS'
      'WHERE'
      '        UID = :OLD_UID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO DOCUMENTS('
      '    UID,'
      '    KOD,'
      '    DT,'
      '    SUPPLIER,'
      '    NUM_NAKL,'
      '    TOTAL_SUM,'
      '    IDPEOPLES,'
      '    SUBEKT,'
      '    IDDOGOVOR,'
      '    PROVOD,'
      '    Operacii_type'
      ')'
      'VALUES('
      '    :UID,'
      '    :KOD,'
      '    :DT,'
      '    (select first 1 id from contragents where c_type = 0),'
      '    :NUM_NAKL,'
      '    :TOTAL_SUM,'
      '    :IDPEOPLES,'
      '    (select first 1 id from tbsubekt where binner = 1),'
      '    :IDDOGOVOR,'
      '    :ACCEPT,'
      '    :Operacii_type'
      ')')
