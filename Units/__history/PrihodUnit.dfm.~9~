      '    d.TOTAL_SUM,'
      '    d.IDPEOPLES,'
      '    d.SUBEKT,'
      '    (select name from tbSubekt where id = d.SUBEKT) as SKLAD,'
      '    d.IDDOGOVOR,  '
      '    provod AS accept,'
      
        '    (select sum(cena_sales*kolvo) from d_lines where idDocument ' +
        '= d.uid) as sum_real,'
      '    (select name from peoples where id = d.idpeoples) as otvet '
      'FROM'
      '    DOCUMENTS d '
      'WHERE(  d.IDSTATION = 0'
      'and d.Operacii_type = 1'
      'and provod = 0'
      '     ) and (     D.UID = :OLD_UID'
      '     )'
      '    ')
    SelectSQL.Strings = (
