  object D_LINES: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE D_LINES'
      'SET '
      '    IDDOCUMENT = :IDDOCUMENT,'
      '    PRODUCT = :PRODUCT,'
      '    SUM_NAKL = :CENA_SALES * :KOLVO,'
      '    KOLVO = :KOLVO,'
      '    CENA_SALES = :CENA_SALES,'
      '    INO = :INO'
      'WHERE'
      '    UID = :OLD_UID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    D_LINES'
      'WHERE'
      '        UID = :OLD_UID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO D_LINES('
      '    UID,'
      '    IDDOCUMENT,'
      '    PRODUCT,'
      '    SUM_NAKL,'
      '    KOLVO,'
      '    CENA_SALES,'
      '    INO'
      ')'
      'VALUES('
      '    :UID,'
      '    :IDDOCUMENT,'
      '    :PRODUCT,'
      '    :KOLVO * (select mcena from d_product where id = :PRODUCT),'
      '    :KOLVO,'
      '    (select mcena from d_product where id = :PRODUCT),'
      '    :INO'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    l.UID,'
      '    l.IDDOCUMENT,'
      '    l.PRODUCT,'
      '    l.SUM_NAKL,'
      '    l.KOLVO,'
      '    l.cena_sales,'
      '    l.ino'
      'FROM'
      '    D_LINES l inner join D_PRoduct p on(l.product=p.id)'
      'WHERE(  IDDOCUMENT = :mas_UID'
      '     ) and (     L.UID = :OLD_UID'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    l.UID,'
      '    l.IDDOCUMENT,'
      '    l.PRODUCT,'
      '    l.SUM_NAKL,'
      '    l.KOLVO,'
      '    l.cena_sales,'
      '    l.ino'
      'FROM'
      '    D_LINES l inner join D_PRoduct p on(l.product=p.id)'
      'WHERE IDDOCUMENT = :mas_UID'
      'ORDER BY INO')
    Transaction = DM.ReadTransaction
    Database = DM.DB
    UpdateTransaction = DM.EditTransaction
    AutoCommit = True
    DataSource = DocDataSource
    DefaultFormats.DateTimeDisplayFormat = 'dd.mm.yyyy hh:mm'
    DefaultFormats.DisplayFormatTime = 'hh:mm'
