  object tbStore: TpFIBDataSet
    RefreshSQL.Strings = (
      
        'select f.idProduct, f.product, sum(f.kolvo) as kolvo, f.agent, c' +
        'ase su.ID when -1 then null else su.ID end as SUBEKT'
      'from (SELECT d.idPRODUCT, p.name as PRODUCT, d.idSubekt,'
      'd.kolvo as KOLVO,'
      '(select name from contragents where id = P.SUPPLIER) as agent'
      'from'
      'dimension d'
      'inner join D_Product p on(d.idproduct=p.id)'
      'where d.idSubekt = coalesce(:mas_SUBEKT, d.idSubekt)'
      'and P.PID in (select ID'
      'from D_CLASSIF where ID = :mas_ID or PID = :mas_ID)'
      'and p.bdel = 0'
      ') f inner join tbSubekt su on(f.idSubekt = su.id)'
      'where su.ID = coalesce(:mas_SUBEKT, su.id)'
      'group by f.idProduct, f.product, f.agent, subekt;')
    SelectSQL.Strings = (
      
        'select f.idProduct, f.product, sum(f.kolvo) as kolvo, f.agent, c' +
        'ase su.ID when -1 then null else su.ID end as SUBEKT'
      'from (SELECT d.idPRODUCT, p.name as PRODUCT, d.idSubekt,'
      'd.kolvo as KOLVO,'
      '(select name from contragents where id = P.SUPPLIER) as agent'
      'from'
      'dimension d'
      'inner join D_Product p on(d.idproduct=p.id)'
      'where d.idSubekt = coalesce(:mas_SUBEKT, d.idSubekt)'
      'and P.PID in (select ID'
      'from D_CLASSIF where ID = :mas_ID or PID = :mas_ID)'
      'and p.bdel = 0'
      ') f inner join tbSubekt su on(f.idSubekt = su.id)'
      'where su.ID = coalesce(:mas_SUBEKT, su.id)'
      'group by f.idProduct, f.product, f.agent, subekt;')
    Transaction = DM.ReadTransaction
    Database = DM.DB
    AutoCommit = True
    DataSource = groupSource
    DefaultFormats.DateTimeDisplayFormat = 'dd.mm.yyyy hh:mm'
    DefaultFormats.DisplayFormatTime = 'hh:mm'
    Left = 376
