  object Documents: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE DOCUMENTS'
      'SET '
      '    KOD = :KOD,'
      '    DT = :DT,'
      '    SUPPLIER = :SUPPLIER,'
      '    NUM_NAKL = :NUM_NAKL,'
      '    TOTAL_SUM = :TOTAL_SUM,'
      '    IDPEOPLES = :IDPEOPLES,'
      '    SUBEKT = :SUBEKT,'
      '    IDDOGOVOR = :IDDOGOVOR,'
      '    PROVOD = :ACCEPT'
      'WHERE'
      '    UID = :OLD_UID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    DOCUMENTS'
      'WHERE'
      '        UID = :OLD_UID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO DOCUMENTS('
      '    UID,'
      '    KOD,'
      '    DT,'
      '    SUPPLIER,'
      '    NUM_NAKL,'
      '    TOTAL_SUM,'
      '    IDPEOPLES,'
      '    SUBEKT,'
      '    IDDOGOVOR,'
      '    PROVOD,'
      '    Operacii_type'
      ')'
      'VALUES('
      '    :UID,'
      '    :KOD,'
      '    :DT,'
      '    (select first 1 id from contragents where c_type = 0),'
      '    :NUM_NAKL,'
      '    :TOTAL_SUM,'
      '    :IDPEOPLES,'
      '    (select first 1 id from tbsubekt where binner = 1),'
      '    :IDDOGOVOR,'
      '    :ACCEPT,'
      '    :Operacii_type'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    d.UID,'
      '    d.KOD,'
      '    d.dt as dt,'
      '    d.SUPPLIER,'
      
        '    (Select name from contragents where id = d.supplier) as post' +
        'avschik,'
      '    d.NUM_NAKL,'
      '    d.TOTAL_SUM,'
      '    d.IDPEOPLES,'
      '    d.SUBEKT,'
      '    (select name from tbSubekt where id = d.SUBEKT) as SKLAD,'
      '    d.IDDOGOVOR,  '
      '    provod AS accept,'
      
        '    (select sum(cena_sales*kolvo) from d_lines where idDocument ' +
        '= d.uid) as sum_real,'
      '    (select name from peoples where id = d.idpeoples) as otvet '
      'FROM'
      '    DOCUMENTS d '
      'WHERE(  d.IDSTATION = 0'
      'and d.Operacii_type = 1'
      'and provod = 0'
      '     ) and (     D.UID = :OLD_UID'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    d.UID,'
      '    d.KOD,'
      '    d.dt as dt,'
      '    d.SUPPLIER,'
      
