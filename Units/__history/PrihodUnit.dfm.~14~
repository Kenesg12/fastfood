  object tbStrore_itmes: TpFIBDataSet
    SelectSQL.Strings = (
      
        'execute block (mas_IDPRODUCT integer = :mas_IDPRODUCT, mas_SUBEK' +
        'T integer = :mas_subekt, s timestamp = :s)'
      
        'returns (dt timestamp, tip_oper varchar(255), kolvo double preci' +
        'sion, cena d_currency, osnov integer, kassa varchar(255),'
      'sklad varchar(255), rem double precision)'
      'as'
      'declare variable u d_uuid;'
      'declare variable e timestamp;'
      'declare variable su integer;'
      'begin'
      'if (:s is null) then'
      ' begin'
      
        '  select first 1 d.uid, d.dt from documents d inner join d_lines' +
        ' l on(d.uid = l.iddocument)'
      '  where d.operacii_type = 5 and d.subekt = :mas_subekt'
      '  and d.provod = 1 and l.product = :mas_idproduct'
      '  order by dt desc into :u, :s;'
      ' end'
      'if (:s is null) then'
      ' s = '#39'12.12.2000'#39';'
      
        'for select g.dt, g.tip_oper, g.kolvo, g.cena, g.osnov, g.kassa, ' +
        'g.sklad, g.rem'
      
        'from (select 0 as id, d.dt, '#39#1053#1072#1095'. '#1086#1089#1090#1072#1090#1086#1082#39' as tip_oper, l.kolvo,' +
        ' l.cena_zakup as cena, d.kod as osnov,'
      '(SELECT NAME FROM TBSTATION WHERE ID = D.IDSTATION) AS KASSA,'
      
        '(select name from tbSubekt where id = d.subekt) as sklad, coales' +
        'ce(rs.remains, 0) + coalesce(rs.kolvo, 0) as rem'
      'from documents d inner join d_lines l on(d.uid = l.iddocument)'
      'inner join resources rs on(l.uid = rs.idosnov)'
      'where l.product = :mas_IDPRODUCT and d.uid = :u'
      'union all'
      
        'select r.id, d.dt, o.name, r.kolvo, iif(r.kolvo = 0, 0, r.amount' +
        '/r.kolvo), d.kod, (SELECT NAME FROM TBSTATION WHERE ID = D.IDSTA' +
        'TION),'
      
        '(select name from tbSubekt where id = di.idSubekt), coalesce(r.r' +
        'emains, 0) + coalesce(r.kolvo, 0)'
      'from documents d inner join d_lines l on(d.uid = l.iddocument)'
      'inner join resources r on(l.uid = r.idosnov)'
      'inner join dimension di on(r.pid = di.id)'
      'inner join tbOperacii_type o on(d.operacii_type = o.id)'
      'where di.idproduct = :mas_IDPRODUCT'
      'and di.idsubekt = coalesce(:mas_SUBEKT, di.idSubekt)'
      'and d.dt > :s and d.uid <> coalesce(:u, '#39#39')) g'
      'order by g.dt'
      
        'into :dt, :tip_oper, :kolvo, :cena, :osnov, :kassa, :sklad, :rem' +
        ' do'
      'suspend;'
      ''
      'end')
    Database = DM.DB
    DataSource = StoreSource
    DefaultFormats.DateTimeDisplayFormat = 'dd.mm.yyyy hh:mm'
    DefaultFormats.NumericDisplayFormat = '#,##0.###'
    DefaultFormats.DisplayFormatTime = 'hh:mm'
    Left = 648
    Top = 312
    WaitEndMasterScroll = True
    dcForceOpen = True
    object tbStrore_itmesDT: TFIBDateTimeField
      FieldName = 'DT'
    end
    object tbStrore_itmesTIP_OPER: TFIBStringField
      FieldName = 'TIP_OPER'
      Size = 255
      EmptyStrToNull = True
    end
    object tbStrore_itmesKOLVO: TFIBFloatField
      FieldName = 'KOLVO'
      DisplayFormat = '0.#####'
    end
    object tbStrore_itmesOSNOV: TFIBIntegerField
      FieldName = 'OSNOV'
    end
    object tbStrore_itmesKASSA: TFIBStringField
      FieldName = 'KASSA'
      Size = 255
      EmptyStrToNull = True
    end
    object tbStrore_itmesSKLAD: TFIBStringField
      FieldName = 'SKLAD'
      Size = 255
      EmptyStrToNull = True
    end
    object tbStrore_itmesCENA: TCurrencyField
      FieldName = 'CENA'
    end
    object tbStrore_itmesREM: TFIBFloatField
      FieldName = 'REM'
      DisplayFormat = '0.###'
    end
  end
  object DataSource2: TDataSource
    DataSet = tbStrore_itmes
    Left = 728
