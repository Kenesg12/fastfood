  object Accept: TpFIBStoredProc
    Transaction = DM.EditTransaction
    Database = DM.DB
    SQL.Strings = (
      'execute block (DOCUM d_uuid = :DOCUM)'
      'as'
      'begin'
      'EXECUTE PROCEDURE ACCEPT_PRIHOD (:DOCUM);'
      ''
      'update documents '
      
        'set total_sum = (select sum(sum_nakl) from d_lines where iddocum' +
        'ent = :DOCUM)'
      'where uid = :DOCUM;'
      'end')
    StoredProcName = 'ACCEPT_PRIHOD'
    Left = 216
    Top = 272
    qoAutoCommit = True
    qoStartTransaction = True
    qoTrimCharFields = True
  end
  object Doc_list: TpFIBDataSet
    RefreshSQL.Strings = (
      'SELECT'
      '    d.UID,'
      '    d.KOD,'
      '    cast(d.dt as date) as dt,'
      '    d.SUPPLIER,'
      
        '    (Select name from contragents where id = d.supplier) as post' +
        'avschik,'
      '    d.NUM_NAKL,'
      '    d.TOTAL_SUM,'
      '    d.IDPEOPLES,'
      '    d.SUBEKT,'
      '    d.Operacii_type,'
      '    (select name from tbSubekt where id = d.SUBEKT) as SKLAD,'
      '    d.IDDOGOVOR,  '
      '    provod AS accept,'
      
        '    (select sum(cena_sales*kolvo) from d_lines where idDocument ' +
        '= d.uid) as sum_real,'
      '    (select name from peoples where id = d.idpeoples) as otvet '
      'FROM'
      '    DOCUMENTS d '
      'WHERE(  d.IDSTATION = 0'
      'and d.Operacii_type = 1'
      'and provod = 1'
      '     ) and (     D.UID = :OLD_UID'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    d.UID,'
      '    d.KOD,'
      '    cast(d.dt as date) as dt,'
      '    d.SUPPLIER,'
      
        '    (Select name from contragents where id = d.supplier) as post' +
        'avschik,'
      '    d.NUM_NAKL,'
      '    d.TOTAL_SUM,'
      '    d.IDPEOPLES,'
      '    d.SUBEKT,'
      '    d.Operacii_type,'
      '    (select name from tbSubekt where id = d.SUBEKT) as SKLAD,'
      '    d.IDDOGOVOR,  '
      '    provod AS accept,'
      
        '    (select sum(cena_sales*kolvo) from d_lines where idDocument ' +
        '= d.uid) as sum_real,'
      '    (select name from peoples where id = d.idpeoples) as otvet '
      'FROM'
      '    DOCUMENTS d '
      'WHERE d.IDSTATION = 0'
      'and d.Operacii_type = 1'
      'and provod = 1')
    Transaction = DM.ReadTransaction
    Database = DM.DB
    UpdateTransaction = DM.EditTransaction
    DefaultFormats.DateTimeDisplayFormat = 'dd.mm.yyyy hh:mm'
    DefaultFormats.DisplayFormatTime = 'hh:mm'
    Left = 136
