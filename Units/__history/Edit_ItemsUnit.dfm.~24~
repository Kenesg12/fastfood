  object checkWeght: TFDQuery
    Connection = DM.FDB
    Transaction = DM.trRead
    UpdateOptions.AssignedValues = [uvEDelete, uvEInsert, uvEUpdate]
    UpdateOptions.EnableDelete = False
    UpdateOptions.EnableInsert = False
    UpdateOptions.EnableUpdate = False
    SQL.Strings = (
      'select p.id, p.mcena, p.name'
      
        'from product_classificators k join d_product p on k.idproduct = ' +
        'p.id'
      'where k.idclassificators = ('
      'select c.idclassificators'
      
        'from product_classificators c join d_product s on c.idproduct = ' +
        's.id'
      'where c.idproduct = :id'
      'and s.isweight = 0)'
      'and p.isweight = 1'
      'rows 1')
    Left = 240
    Top = 336
    ParamData = <
      item
        Name = 'ID'
        DataType = ftInteger
        ParamType = ptInput
      end>
    object checkWeghtID: TIntegerField
      FieldName = 'ID'
      Origin = 'ID'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
      Required = True
    end
    object checkWeghtMCENA: TCurrencyField
      FieldName = 'MCENA'
      Origin = 'MCENA'
    end
    object checkWeghtNAME: TWideStringField
      FieldName = 'NAME'
      Origin = 'NAME'
      Size = 150
    end
  end
end
