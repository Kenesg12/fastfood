    <TfrxFDQuery Name="FDQuery1" UserName="IBXQuery1" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select p.name as product, p.id as idproduct, e.name as edizm, sum(pr.kolvo) as kolvo_prihod, sum(pr.amount) as amount_prihod,&#13;&#10;sum(ra.kolvo) as kolvo_rashod, sum(ra.amount) as amount_rashod,&#13;&#10;sum(nach.kolvo) as kolvo_nach, sum(nach.amount) as amount_nach, avg(nach.cena) as cena_nach,&#13;&#10;sum(kon.kolvo) as kolvo_kon, sum(kon.amount) as amount_kon, avg(kon_sls.cena_sales) as cena_sales,&#13;&#10;avg(kon.cena) as cena_kon&#13;&#10;from d_product p&#13;&#10;inner join tbEdizm e on(p.idedizm = e.id)&#13;&#10;left join (select m.idProduct, sum(abs(s.Kolvo)) as kolvo, sum(abs(s.amount)) as amount from resources s inner join dimension m on(s.pid = m.id)&#13;&#10;inner join d_lines l on(s.idosnov = l.uid) inner join documents d on(l.iddocument = d.uid)&#13;&#10;where d.dt between :s and :e and s.kolvo &#62; 0 and m.idsubekt = iif(:ss = 0, m.idSubekt, :ss) group by m.idProduct) pr on(p.id = pr.idProduct)&#13;&#10;left join (select m.idProduct, sum(abs(s.Kolvo)) as kolvo, sum(abs(s.amount)) as amount from resources s inner join dimension m on(s.pid = m.id)&#13;&#10;inner join d_lines l on(s.idosnov = l.uid) inner join documents d on(l.iddocument = d.uid)&#13;&#10;where d.dt between :s and :e and s.kolvo &#60; 0 and m.idsubekt = iif(:ss = 0, m.idSubekt, :ss) group by m.idProduct) ra on(p.id = ra.idProduct)&#13;&#10;left join (select m.idProduct, sum(r.Kolvo) as kolvo, sum(r.amount) as amount, iif(sum(r.Kolvo) = 0, 0, sum(r.amount)/sum(r.Kolvo)) as cena from resources r inner join dimension m&#13;&#10;on (r.pid = m.id) inner join d_lines li on(r.idosnov = li.uid) inner join documents di on(li.iddocument = di.uid) where di.dt &#60; :s and m.idsubekt = iif(:ss = 0, m.idsubekt, :ss) group by m.idproduct) nach on(p.id = nach.idProduct)&#13;&#10;left join (select m.idProduct, sum(r.Kolvo) as kolvo, sum(r.amount) as amount, iif(sum(r.Kolvo) = 0, 0, sum(r.amount)/sum(r.Kolvo)) as cena from resources r inner join dimension m&#13;&#10;on (r.pid = m.id) inner join d_lines li on(r.idosnov = li.uid) inner join documents di on(li.iddocument = di.uid) where di.dt &#60;= :e and m.idsubekt = iif(:ss = 0, m.idsubekt, :ss) group by m.idproduct) kon on(p.id = kon.idProduct)&#13;&#10;left join (select avg(r.amount_sales / -r.kolvo) as cena_sales, m.idproduct from resources r inner join dimension m&#13;&#10;on (r.pid = m.id) inner join d_lines li on(r.idosnov = li.uid) inner join documents di on(li.iddocument = di.uid) where di.dt &#60;= :e and m.idsubekt = iif(:ss = 0, m.idsubekt, :ss) and di.operacii_type = 2 group by m.idproduct) kon_sls on(p.id = kon_sls.idProduct)&#13;&#10;left join product_classificators k on(p.id = k.idproduct) and k.idclassificators = iif(:su = 0, k.idclassificators, :su)&#13;&#10;where p.id in (select distinct dm.idProduct from dimension dm inner join resources r on(dm.id = r.pid)&#13;&#10;inner join d_lines ls on(r.idosnov = ls.uid) inner join documents doc on(ls.iddocument = doc.uid)&#13;&#10;where dm.idSubekt = iif(:ss = 0, dm.idSUbekt, :ss) and doc.dt between :s and :e)&#13;&#10;group by p.name, p.id, e.name&#13;&#10;order by p.name                                  &#13;&#10;&#13;&#10;  " PropData=""/>
