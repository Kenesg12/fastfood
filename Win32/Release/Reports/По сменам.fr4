    <TfrxFDQuery Name="FDQuery1" UserName="FDQuery1" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select s.id, cast(s.dtstart as date) as dt, (select k.amount from tbkassa k where k.idoper = 5 and k.idsmeny = s.id) as nach,&#13;&#10;(select sum(l.sum_nakl) from tbkassa k inner join d_lines l on(k.iddoc = l.iddocument) inner join d_product p on(l.product = p.id) where k.idsmeny = s.id and coalesce(p.pid, -1) = -1 and k.idoper = 2) as tarif,&#13;&#10;(select sum(l.sum_nakl) from tbkassa k inner join d_lines l on(k.iddoc = l.iddocument) inner join d_product p on(l.product = p.id) where k.idsmeny = s.id and p.pid &#62; -1 and k.idoper = 2) as bar,&#13;&#10;(select sum(k.amount) from tbkassa k where k.idoper = 1  and k.idsmeny = s.id) as prihod,&#13;&#10;(select sum(k.amount) from tbkassa k where k.idoper = 3 and k.idsmeny = s.id) as vnesenie,&#13;&#10;(select sum(k.amount) from tbkassa k where k.idoper = 4 and k.idsmeny = s.id and k.iddoc = '1') as izyat,&#13;&#10;(select sum(k.amount) from tbkassa k where k.idoper = 4 and k.idsmeny = s.id and k.iddoc = '2') as inkas,&#13;&#10;(select sum(k.amount) from tbkassa k where k.idoper = 4 and k.idsmeny = s.id and k.iddoc = '3') as avans&#13;&#10;from tbsmeny s&#13;&#10;where extract(year from s.dtstart) = :y&#13;&#10;and extract(month from s.dtstart) = :m&#13;&#10;order by s.id desc                                               " PropData=""/>
